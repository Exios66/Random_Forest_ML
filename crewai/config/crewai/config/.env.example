# CrewAI Configuration
# Copy this file to .env and fill in your actual API keys

# OpenAI API Key (required for OpenAI models)
OPENAI_API_KEY=your_openai_api_key_here

# Serper.dev API Key (required for web search functionality)
SERPER_API_KEY=your_serper_api_key_here

# Optional: Local model configuration (for Ollama)
# Uncomment and configure if using local models instead of OpenAI
# OPENAI_API_BASE=http://localhost:11434/v1
# OPENAI_MODEL_NAME=openhermes
# OPENAI_API_KEY=sk-placeholder-key  # Dummy key for local models

# Optional: Alternative LLM providers
# ANTHROPIC_API_KEY=your_anthropic_key_here
# GROQ_API_KEY=your_groq_key_here

# CrewAI Settings
CREWAI_VERBOSE=True
CREWAI_PROCESS=sequential
